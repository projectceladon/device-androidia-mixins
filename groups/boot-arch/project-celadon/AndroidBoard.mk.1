{{^use_cic}}
GPT_INI2BIN := ./$(INTEL_PATH_COMMON)/gpt_bin/gpt_ini2bin.py

$(BOARD_GPT_BIN): $(BOARD_GPT_INI)
	$(hide) $(GPT_INI2BIN) $< > $@
	$(hide) echo GEN $(notdir $@)

$(PRODUCT_OUT)/efi/startup.nsh: $(TARGET_DEVICE_DIR)/{{_extra_dir}}/$(@F)
	$(ACP) $(TARGET_DEVICE_DIR)/{{_extra_dir}}/$(@F) $@
	sed -i '/#/d' $@
